{
    "name": "thiagof/laravelrpc",
    "description": "JsonRPC Client/Server services for Laravel 5",
    "keywords": ["laravel", "jsonrpc", "rpc", "json-rpc", "client", "server"],
    "license": "MIT",
    "authors": [
        {
            "name": "Thiago Fernandes",
            "email": "thiago@internetbudi.com.br"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/matasarei/JsonRPC"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.0",
        "matasarei/json-rpc": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "laravel/laravel": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Thiagof\\LaravelRPC\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "stable"
}
